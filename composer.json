{
  "name": "ninthspace/action",
  "description": "",
  "keywords": [
    "ninthspace",
    "action"
  ],
  "homepage": "https://github.com/ninthspace/action",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Aves",
      "email": "chris@junctionbox.com",
      "role": "Developer"
    }
  ],
  "require": {
  },
  "require-dev": {
    "php": "^7.4",
    "nunomaduro/collision": "^5.0",
    "pestphp/pest": "^0.1.5",
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-master",
    "spatie/phpunit-watcher": "^1.22"
  },
  "autoload": {
    "psr-4": {
      "Ninthspace\\Action\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ninthspace\\Action\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
